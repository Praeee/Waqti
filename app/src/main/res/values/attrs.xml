<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AppBar">
        <attr name="leftImage" format="reference" />
        <attr name="rightImage" format="reference" />
        <attr name="hint" format="string" />
        <attr name="leftClickOpensDrawer" format="boolean" />
        <attr name="rightClickOpensMenu" format="reference" />
        <attr name="hasRightImage" format="boolean" />
        <attr name="editable" format="boolean" />
        <attr name="text" format="string" />
        <attr name="isMultiLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PropertyCard">
        <attr name="title" format="string" />
    </declare-styleable>

    <declare-styleable name="EditTextView">
        <attr name="isEditable" format="boolean" />
        <attr name="isMultiline" format="boolean" />
    </declare-styleable>

</resources>